    function GetIdAnswer(id1) {
        $.ajax({
            url: '/Admin/GetIdAnswer?id=' + id1,
            type: 'GET',
            success: function (response) {
                var data1 = response.Data;
                console.log("hello Answer");
                console.log(response);
                var html = "<table class='table table-hoverable'>" +
                    "<thead>" +
                    "<tr>" +
                    "<th style='text-align: center'>Answer</th>" +
                    "<th style='text-align: center'>False/True</th>" +
                    "</tr>" +
                    "</thead>" +
                    "<tbody>";

                for (var i = 0; i < data1.length; i++) {
                    html += "<tr>" +
                        "<td style='text-align: center'>" + data1[i].title + "</td>" +
                        "<td style='text-align: center'>";

                    if (data1[i].iscorrect == "0") {
                        html += "<p style='color:red'><b>False</b></p>";
                    } else if (data1[i].iscorrect == "1") {
                        html += "<p style='color:green'><b>True</b></p>";
                    }

                    html += "</td>" +
                        "</tr>";
                }

                html += "</tbody>" +
                    "</table>";
                var divHtml = document.getElementById("content_new");
                divHtml.innerHTML = html;
                // Xử lý kết quả trả về từ server
                console.log(response);
            },
            error: function (xhr, status, error) {
                // Xử lý lỗi
                console.error(error);
            }
        });
    }
